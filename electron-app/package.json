    "vscode-builtin-markdown": "https://open-vsx.org/api/vscode/markdown/1.62.3/file/vscode.markdown-1.62.3.vsix",
    "vscode-builtin-xml": "https://open-vsx.org/api/vscode/xml/1.62.3/file/vscode.xml-1.62.3.vsix",
    "vscode-builtin-yaml": "https://open-vsx.org/api/vscode/yaml/1.62.3/file/vscode.yaml-1.62.3.vsix",
    "vscode-builtin-npm": "https://open-vsx.org/api/vscode/npm/1.62.3/file/vscode.npm-1.62.3.vsix",
    "vscode-builtin-html": "https://open-vsx.org/api/vscode/html/1.62.3/file/vscode.html-1.62.3.vsix",
