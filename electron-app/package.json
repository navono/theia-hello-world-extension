{
  "private": true,
  "name": "electron-app",
  "version": "0.0.0",
  "main": "scripts/theia-electron-main.js",
  "scripts": {
    "prepare": "yarn build && yarn download:plugins",
    "clean": "theia clean && rimraf node_modules",
    "clean:dist": "rimraf dist",
    "build": "theia rebuild:electron && yarn bundle",
    "bundle": "theia build",
    "watch": "concurrently -n compile,bundle \"theiaext watch --preserveWatchOutput\" \"theia build --watch --mode development\"",
    "start": "electron scripts/theia-electron-main.js",
    "start:debug": "yarn start --log-level=debug",
    "package": "yarn clean:dist && electron-builder -c.mac.identity=null --publish never",
    "deploy": "yarn clean:dist && electron-builder -c.mac.identity=null --publish always",
    "package:preview": "yarn clean:dist && electron-builder -c.mac.identity=null --dir",
    "update:checksum": "ts-node scripts/update-checksum.ts",
    "update:next": "ts-node ../../scripts/update-theia-to-next.ts",
    "download:plugins": "theia download:plugins",
    "nls": "theia nls-extract -o lang.json"
  },
  "engines": {
    "node": ">=12.14.1 <13"
  },
  "dependencies": {
    "@theia/console": "^1.21.0",
    "@theia/core": "latest",
    "@theia/debug": "^1.21.0",
    "@theia/editor": "latest",
    "@theia/electron": "latest",
    "@theia/file-search": "^1.21.0",
    "@theia/filesystem": "latest",
    "@theia/getting-started": "^1.21.0",
    "@theia/markers": "latest",
    "@theia/messages": "latest",
    "@theia/monaco": "latest",
    "@theia/navigator": "latest",
    "@theia/output": "^1.21.0",
    "@theia/plugin-ext-vscode": "^1.21.0",
    "@theia/preferences": "latest",
    "@theia/process": "latest",
    "@theia/property-view": "^1.21.0",
    "@theia/search-in-workspace": "^1.21.0",
    "@theia/task": "^1.21.0",
    "@theia/terminal": "latest",
    "@theia/vsx-registry": "^1.21.0",
    "@theia/workspace": "latest",
    "theia-hello-world-extension": "0.1.0"
  },
  "devDependencies": {
    "@theia/cli": "latest",
    "electron-builder": "22.10.5"
  },
  "theia": {
    "target": "electron",
    "frontend": {
      "config": {
        "applicationName": "Theia Preload Example"
      }
    },
    "backend": {
      "config": {
        "startupTimeout": -1,
        "resolveSystemPlugins": false
      }
    }
  },
  "generator": {
    "config": {
      "preloadTemplate": "../preload.html"
    }
  },
  "theiaPluginsDir": "plugins",
  "theiaPlugins": {
    "vscode-builtin-javascript": "https://open-vsx.org/api/vscode/javascript/1.62.3/file/vscode.javascript-1.62.3.vsix",
    "vscode-builtin-typescript": "https://open-vsx.org/api/vscode/typescript/1.62.3/file/vscode.typescript-1.62.3.vsix",
    "vscode-builtin-cpp": "https://open-vsx.org/api/vscode/cpp/1.62.3/file/vscode.cpp-1.62.3.vsix",
    "vscode-builtin-python": "https://open-vsx.org/api/vscode/python/1.62.3/file/vscode.python-1.62.3.vsix",
    "vscode-builtin-java": "https://open-vsx.org/api/vscode/java/1.62.3/file/vscode.java-1.62.3.vsix",
    "vscode-builtin-ipynb": "https://open-vsx.org/api/vscode/ipynb/1.62.3/file/vscode.ipynb-1.62.3.vsix",
    "vscode-builtin-json": "https://open-vsx.org/api/vscode/json/1.62.3/file/vscode.json-1.62.3.vsix",
