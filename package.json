{
  "private": true,
  "scripts": {
    "prepare": "lerna run prepare && yarn download:plugins",
    "rebuild": "yarn rebuild:browser && yarn rebuild:electron",
    "rebuild:browser": "theia rebuild:browser",
    "rebuild:electron": "theia rebuild:electron",
    "start:browser": "yarn rebuild:browser && yarn --cwd examples/browser-app start",
    "start:electron": "yarn rebuild:electron && yarn --cwd examples/electron-app start",
    "download:plugins": "theia download:plugins -p=true",
    "watch": "lerna run --parallel watch",
    "lint": "lerna run lint"
  },
  "devDependencies": {
    "@theia/cli": "^1.23.0",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
