{
  "name": "ecsnext-prototype",
  "keywords": [
    "theia-extension"
  ],
  "version": "0.0.1",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest --config configs/jest.config.ts",
    "lint": "eslint --fix --ext .tsx,.ts ./src"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@theia/core": "latest",
    "antd": "^4.18.8",
    "ecsnext-base": "^0.1.0",
    "http-proxy": "^1.18.1",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.7",
    "@types/http-proxy": "^1.17.8",
    "@types/jest": "^26.0.20",
    "eslint-plugin-no-null": "^1.0.2",
    "jest": "^26.6.3",
    "rimraf": "latest",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "latest"
  },
  "eslintIgnore": [
    "*.spec.ts",
    "*.test.ts"
  ],
  "theiaExtensions": [
    {
      "frontend": "lib/browser/ecsnext-prototype-frontend-module",
      "backend": "lib/node/backend-module"
    }
  ]
}
