{
  "name": "theia-hello-world-extension",
  "keywords": [
    "theia-extension"
  ],
  "version": "0.1.0",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@eclipse-emfcloud/theia-tree-editor": "next",
    "@theia/core": "latest",
    "@theia/editor": "^1.21.0",
    "@theia/monaco": "^1.21.0",
    "@theia/outline-view": "^1.21.0",
    "@theia/property-view": "^1.21.0",
    "@theia/task": "^1.21.0",
    "@theia/terminal": "^1.21.0",
    "highlight.js": "^11.3.1",
    "markdown-it": "^12.3.0",
    "three": "^0.137.5",
    "ecsnext-base": "0.1.0"
  },
  "devDependencies": {
    "@types/highlight.js": "^10.1.0",
    "@types/markdown-it": "^12.2.3",
    "@types/three": "^0.137.0",
    "rimraf": "latest",
    "typescript": "latest"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
